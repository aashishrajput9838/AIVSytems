{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "runtime": "nodejs22"
    },
    {
      "source": "codebase2",
      "codebase": "codebase2",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "emulators": {
    "functions": {
      "port": 5004
    },
    "firestore": {
      "port": 8082
    },
    "ui": {
      "enabled": true,
      "port": 4004
    },
    "hub": {
      "port": 4404
    },
    "logging": {
      "port": 4504
    }
  }
}